{
  "name": "nuxtjs-with-typescript-example-template",
  "version": "1.0.0",
  "description": "Nuxt.js with TypeScript example template",
  "private": true,
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "lint:style": "stylelint **/*.{css,scss,sass,html,vue}",
    "lint:style:fix": "stylelint **/*.{css,scss,sass,html,vue} --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
